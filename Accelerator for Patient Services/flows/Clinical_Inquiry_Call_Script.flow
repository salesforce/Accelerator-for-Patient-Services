    <decisions>
        <name>General_or_Scheduling</name>
        <label>General or Scheduling</label>
        <locationX>446</locationX>
        <locationY>278</locationY>
        <defaultConnector>
            <targetReference>General_Call_Script</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>General</defaultConnectorLabel>
        <rules>
            <name>Scheduling</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordId.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Scheduling</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Scheduling_Call_Script</targetReference>
            </connector>
            <label>Scheduling</label>
        </rules>
    </decisions>
