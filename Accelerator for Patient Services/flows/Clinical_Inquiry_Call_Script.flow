        </defaultConnector>
        <defaultConnectorLabel>General or Scheduling</defaultConnectorLabel>
        <rules>
            <name>Clinical_Case</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordId.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Clinical</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Clinical_Call_Script</targetReference>
            </connector>
            <label>Clinical Case</label>
        </rules>
    </decisions>
